  <script>
  
    // XML 데이터 요청
  
    const getData1 = ()=>{
      
      const options = {
        method: 'GET'
      }
      
      fetch('/servlet/getData1', options)
        /*.then((response)=>{
          if(response.ok){
            return response.text();
          }
        })*/
        .then(response=>response.text())
        .then(resData=>{
          let domParser = new DOMParser();
          let xmlDoc = domParser.paseFromString(resData, 'application/xml');
          console.log(xmlDoc);
        })
    }
    
    document.getElementById("btn1").addEventListener('click', getData1);
  
  </script>
  
